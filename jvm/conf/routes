GET			/                       todomvc.example.TodoController.home
GET			/api/todos              todomvc.example.TodoController.list
PUT			/api/todos              todomvc.example.TodoController.create
POST		/api/todos/markAll      todomvc.example.TodoController.markAll(completed: Boolean)
POST		/api/todos/clearAll     todomvc.example.TodoController.clearAll

POST		/api/todos/:id          todomvc.example.TodoController.update(id: Long)
DELETE		/api/todos/:id          todomvc.example.TodoController.delete(id: Long)

GET			/assets/*file           controllers.Assets.versioned(path="/public", file: Asset)
